file db/aoFloat64.template

{ pattern {X,           Y,            PORT,       OFFSET,DATA_TYPE, PREC,			EGU,  DESC,	  HOPR, LOPR}
# Control                                                                                          
{"$(P):$(R):", "$(N):otSP"	  ,       "$(O)",      2	,   INT16,     0,     "0.1degC",	"",   6000,  200}
{"$(P):$(R):", "$(N):oSPrr"	  ,       "$(O)",      35	,   INT16,     0, "0.1degC/min",	"",    100,    1}
{"$(P):$(R):", "$(N):oSP_H"	  ,       "$(O)",      111	,   INT16,     0,     "0.1degC",	"",   6000,  251}
{"$(P):$(R):", "$(N):oSP_L"	  ,       "$(O)",      112	,   INT16,     0,	  "0.1degC",	"",	   250,  200}
# Autotune                                                                                         
{"$(P):$(R):", "$(N):otune"	  ,       "$(O)",      270	,	INT16,     0,	         "",	"",		  ,	    }
{"$(P):$(R):", "$(N):odrA"	  ,       "$(O)",      271	,	INT16,     0,	         "",	"",		  ,	    }
{"$(P):$(R):", "$(N):odrAt"	  ,       "$(O)",      100	,	INT16,     0,	         "",	"",		  ,	    }
# PID                                                                                        		   	         
