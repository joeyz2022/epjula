# ai record template for register inputs
record(ai, "$(P):$(R):$(N)_RAW") {
field(DTYP,"asynFloat64")
field(INP,"@asyn($(N))FLOAT32_LE")
field(SCAN,"1 second")
}

# THERMOVAC Transmitter TTR 911 N
# cali V_out = log10 (P_mbar) x 1.286 + 6.143

record(calc, "$(P):$(R):$(N)") {
field(DESC, "Pressure")
field(SCAN,"1 second")
field(CALC, "$(C)")
field(INPA, "$(P):$(R):$(N)_RAW.VAL")
field(EGU, "bar")
}


